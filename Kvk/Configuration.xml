            <JavaListener name="KVK verzoeken"/>
            <!-- url="https://api.kvk.nl/test/api/v1/zoeken?handelsnaam=test" -->
        </Receiver>

        <Pipeline>

            <SenderPipe name="KVK zoeken op handelsnaam" >

                <!-- Configure HttpSender -->
                <HttpSender url="https://api.kvk.nl/test/api/v1/zoeken?handelsnaam=test" methodType="GET" headersParams="apikey">
                    <Param name="apikey" value="l7xx1f2691f2520d487b902f4e0b57a0b197" />
                    <!--<Param name="allowSelfSignedCertificates" value="true" />
                    <Param name="ignoreCertificateExpiredException" value="true" /> -->
                    <!-- Configure HttpSession with keystore and truststore settings -->
                    <!-- <HttpSession
                        keystore = "C:\Program Files\Java\jdk-21\bin\keytool.exe" keystoreType="PKCS12" keystorePassword="changeit"

                        truststore = "C:\Program Files\Java\jdk-21\lib\security\cacerts" truststoreType="PKCS12" truststorePassword="changeit">
                        c++ frankrunner overlaten
                    </HttpSession>-->
                </HttpSender>
                <!--
                <Forward name="success" path="EXIT"/>
                <Forward name="400" path="BadRequest"/>
                <Forward name="401" path="NotAuthenticated"/>
                <Forward name="404" path="NotFound"/>
                <Forward name="500" path="ServerError"/>-->

            </SenderPipe>

            <EchoPipe name="echoPipe">

            </EchoPipe>

            <Exit path="EXIT" state="success"/>

        </Pipeline>

    </Adapter>

